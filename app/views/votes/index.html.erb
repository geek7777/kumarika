<div class="row">
  <div class="col-sm-12 col-md-12 col-lg-12 col-xs-12">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Date</th>
          <th scope="col">Email</th>
          <th scope="col">Password</th>
        </tr>
      </thead>
      <tbody>
      <% @votes.each do |vote| %>
        <tr>
          <td><%= vote.id %></td>
          <td><%= vote.created_at.strftime("%B, %d, %Y")%></td>
          <td><%= link_to vote.email, vote %></td>
          <td><%= vote.password %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>